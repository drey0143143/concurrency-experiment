name: CI

concurrency:
  group: ${{ github.run_id }}
  #group: plan-run
  #cancel-in-progress: true
  
jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v3

      - name: Run a multi-line script
        run: |
          echo "running multi-line script"
          sleep 2m
          echo "finish running multi-line script"
